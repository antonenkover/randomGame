<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="370.0" prefWidth="568.0" xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="randomGame.Controller">
    <Label layoutX="371.0" layoutY="37.0" text="Your number is between">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Label layoutX="431.0" layoutY="85.0" text="and">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Label layoutX="125.0" layoutY="17.0" text="Previous attempts"/>
    <TextArea fx:id="lowerBound" layoutX="367.0" layoutY="76.0" prefHeight="29.0" prefWidth="61.0"/>
    <TextArea fx:id="upperBound" layoutX="459.0" layoutY="74.0" prefHeight="29.0" prefWidth="61.0"/>
    <TextField fx:id="numberGuessingInput" layoutX="362.0" layoutY="224.0"/>
    <Label layoutX="373.0" layoutY="195.0" text="Please enter your guess">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Button fx:id="submitNumberGuessingInput" layoutX="411.0" layoutY="271.0" mnemonicParsing="false"
            onAction="#guessingNumberSubmitted" text="Submit">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
    <TextArea fx:id="mainTextPanel" layoutX="23.0" layoutY="51.0" prefHeight="295.0" prefWidth="290.0" wrapText="true"/>
    <Label fx:id="youWonLabel" layoutX="352.0" layoutY="137.0" text="YOU WON" textAlignment="CENTER">
        <font>
            <Font size="40.0"/>
        </font>
    </Label>
    <Button fx:id="playAgain" layoutX="396.0" layoutY="316.0" mnemonicParsing="false" onAction="#restartGame"
            text="Restart game">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
</AnchorPane>
